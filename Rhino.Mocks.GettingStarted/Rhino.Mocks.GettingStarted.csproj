  <ItemGroup>
    <Content Include="GettingStarted.txt" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\Rhino.Mocks\Rhino.Mocks.csproj" />
  </ItemGroup>
  <ItemGroup>
    <Compile Remove="Documentation\**\*.cs" />
  </ItemGroup>
